<div class="">

  <%= form_with model: current_user, local: true do |f| %>

  <div class="">
    <%= f.label :name, "ユーザー名" %><br />
    <%= f.text_field :name, id:"" %>
  </div>

  <div class="">
    <%= f.label :email, "メールアドレス" %><br />
    <%= f.email_field :email, id:"", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="">
    <%= f.label :category_id, "カテゴリー" %><br />
    <%= f.select :category_id,[["社会人",1],["U-18",2],["U-15",3],["U-12",4]] %>
  </div>

  <div class="">
    <%= f.label :belongs, "所属リーグ" %><br />
    <%= f.text_field :belongs, id:"" %>
  </div>

  <div class="">
    <%= f.label :hometown, "ホームタウン" %><br />
    <%= f.text_field :hometown, id:"" %>
  </div>

  <div class="">
    <%= f.label :homepage, "HP" %><br />
    <%= f.text_field :homepage, id:"" %>
  </div>

  <div class="">
    <%= f.label :image, "チーム画像" %><br />
    <%= f.file_field :image, id:"" %>
  </div>

  <div class="">
    <%= f.submit "更新する", class: :form__btn  %>
  </div>

  <% end %>
</div>